Cell["\<\
If we want to add English-language documentation to our paclet we'd set up \
the paclet like:\
\>", "Text",
 CellChangeTimes->{{3.740505154444125*^9, 3.740505217337819*^9}, {
  3.740505354183242*^9, 
  3.7405053544875*^9}},ExpressionUUID->"02463c1b-5273-4fc4-a779-58dfef3e913d"],

Cell[BoxData[{"none", "\n", "MyPaclet", "\n", 
 RowBox[{" ", 
  RowBox[{"+", " ", 
   RowBox[{"PacletInfo", ".", "m"}]}]}], "\n", 
 RowBox[{" ", "Documentation"}], "\n", 
 RowBox[{"  ", "English"}], "\n", 
 RowBox[{"   ", "ReferencePages"}], "\n", 
 RowBox[{"    ", "Symbols"}], "\n", 
 RowBox[{"     ", 
  RowBox[{"+", " ", 
   RowBox[{"MySymbol1", ".", "nb"}]}]}], "\n", 
 RowBox[{"     ", 
  RowBox[{"+", " ", 
   RowBox[{"MySymbol2", ".", "nb"}]}]}], "\n", 
 RowBox[{"     ", "..."}], "\n", 
 RowBox[{"    ", "Guides"}], "\n", 
 RowBox[{"     ", 
  RowBox[{"+", " ", 
   RowBox[{"MyPaclet", ".", "nb"}]}]}], "\n", 
 RowBox[{"     ", 
  RowBox[{"+", " ", 
   RowBox[{"OtherGuide", ".", "nb"}]}]}], "\n", 
 RowBox[{"     ", "..."}], "\n", 
 RowBox[{"    ", "Tutorials"}], "\n", 
 RowBox[{"     ", 
  RowBox[{"+", " ", 
   RowBox[{"Feature1", ".", "nb"}]}]}], "\n", 
 RowBox[{"     ", 
  RowBox[{"+", " ", 
   RowBox[{"Feature2", ".", "nb"}]}]}], "\n", 
 RowBox[{"     ", "..."}]}], "FencedCode",
 CellChangeTimes->{{3.740505218880088*^9, 3.740505293795285*^9}, 
   3.740527172717678*^9, 
   3.740527918502273*^9},ExpressionUUID->"86f659cb-32b8-445d-9ce4-\
87295f748e96"],

Cell["Then our extension would look like:", "Text",
