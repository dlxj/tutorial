  3.740505370071232*^9}},ExpressionUUID->"bc73f2e0-472c-4930-8977-\
8f835f2c407a"],

Cell[BoxData[
 RowBox[{"{", "\n", "\t", 
  RowBox[{"\"\<Documentation\>\"", ",", "\n", "\t", 
   RowBox[{"\"\<Language\>\"", "\[Rule]", "\"\<English\>\""}], ",", "\n", 
   "\t", 
   RowBox[{"\"\<LinkBase\>\"", "\[Rule]", "\"\<MyPaclet\>\""}], ",", "\n", 
   "\t", 
   RowBox[{"\"\<MainPage\>\"", "\[Rule]", "\"\<Guides/MyPaclet\>\""}]}], "\n",
   "\t", "}"}]], "Code",
 CellChangeTimes->{{3.740502781030562*^9, 3.740502790989842*^9}, {
  3.740502892612664*^9, 3.740502905235506*^9}, {3.740503005442889*^9, 
  3.740503008178445*^9}, {3.740503417313922*^9, 3.740503429817348*^9}, {
  3.740505115957206*^9, 3.740505123075287*^9}, {3.7405053761281023`*^9, 
  3.740505390551013*^9}, {3.740505436808689*^9, 3.74050544359779*^9}, {
