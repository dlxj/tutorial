Cell["Throw / Catch", "Subsubsection",
 CellChangeTimes->{{3.740799538579383*^9, 3.7407995461078777`*^9}, {
  3.740799622180612*^9, 
  3.740799622378998*^9}},ExpressionUUID->"878405e4-074c-45a2-a3ca-\
8b9136751397"],

Cell[TextData[{
 "The ",
 ButtonBox["Throw",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/Throw"],
 " / ",
 ButtonBox["Catch",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/Catch"],
 " mechanism provides a much more robust way to handle errors in Mathematica. \
",
 Cell[BoxData["Catch"], "InlineInput",
  FormatType->"StandardForm",ExpressionUUID->
  "f6afc036-4753-4d5d-8dbc-cfa650cbe902"],
 " takes a second argument that can be a pattern to catch the different error \
tags that ",
 Cell[BoxData["Throw"], "InlineInput",
  FormatType->"StandardForm",ExpressionUUID->
  "ed1d23f2-8d4d-4373-8999-e45a2af54150"],
 " can attach to its results. By doing this we can get deeply-nested \
functions to throw to high-level and be caught only by the handlers we want, \
for instance we can do something like:"
}], "Text",
 CellChangeTimes->{{3.740799555098959*^9, 3.740799591212837*^9}, {
  3.740799625562613*^9, 3.740799634235753*^9}, {3.740799667306651*^9, 
  3.740799747370126*^9}},ExpressionUUID->"a25170b5-9dc3-433e-89bb-\
9c653cf147ef"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"main", "[", "]"}], ":=", "\n", "\t", 
  RowBox[{"NumberForm", "[", 
   RowBox[{
    RowBox[{"Catch", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"sub1", "[", "]"}], ",", " ", 
        RowBox[{"sub2", "[", "]"}], ",", " ", 
        RowBox[{"sub3", "[", "]"}]}], "}"}], ",", " ", 
      RowBox[{"\"\<err1\>\"", "|", "\"\<err2\>\"", "|", "\"\<err3\>\""}]}], 
     "]"}], ",", " ", "3"}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{"sub1", "[", "]"}], ":=", "\n", "\t", 
  RowBox[{
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"#", "<", ".25"}], ",", " ", 
      RowBox[{"Throw", "[", 
       RowBox[{
        RowBox[{"StringForm", "[", 
         RowBox[{"\"\<`` too large\>\"", ",", " ", "#"}], "]"}], ",", " ", 
        "\"\<err1\>\""}], "]"}], ",", " ", "#"}], "]"}], "&"}], "@", 
   RowBox[{"RandomReal", "[", "]"}]}]}], "\n", 
 RowBox[{
  RowBox[{"sub2", "[", "]"}], ":=", "\n", "\t", 
  RowBox[{
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"#", "<", ".5"}], ",", " ", 
      RowBox[{"Throw", "[", 
       RowBox[{
        RowBox[{"StringForm", "[", 
         RowBox[{"\"\<`` two large\>\"", ",", " ", "#"}], "]"}], ",", " ", 
        "\"\<err2\>\""}], "]"}], ",", " ", "#"}], "]"}], "&"}], "@", 
   RowBox[{"RandomReal", "[", "]"}]}]}], "\n", 
 RowBox[{
  RowBox[{"sub3", "[", "]"}], ":=", "\n", "\t", 
  RowBox[{
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"#", "<", ".75"}], ",", " ", 
      RowBox[{"Throw", "[", 
       RowBox[{
        RowBox[{"StringForm", "[", 
         RowBox[{"\"\<`` three large\>\"", ",", " ", "#"}], "]"}], ",", " ", 
        "\"\<err3\>\""}], "]"}], ",", " ", "#"}], "]"}], "&"}], "@", 
   RowBox[{"RandomReal", "[", "]"}]}]}]}], "Code",
 CellChangeTimes->{{3.740799751306444*^9, 3.740799907850637*^9}, {
   3.740799953236463*^9, 3.740799957265286*^9}, 3.740806266216516*^9},
